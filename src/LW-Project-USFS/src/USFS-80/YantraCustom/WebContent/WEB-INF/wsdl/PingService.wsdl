<definitions xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:nwcg="http://nwcg.gov/services/ross/common_types/1.1" xmlns:ro="http://nwcg.gov/services/ross/resource_order/1.1" xmlns:tns="http://nwcg.gov/services/ross/resource_order/wsdl/1.1" xmlns:ns="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd" name="ResourceOrder_WSDL" targetNamespace="http://nwcg.gov/services/ross/resource_order/wsdl/1.1">
	<types>
		<xs:schema version="1.1">
			<xs:import namespace="http://nwcg.gov/services/ross/common_types/1.1" schemaLocation="Ping.xsd"/>
		</xs:schema>
	</types>
	<message name="ServicePingReq">
		<part name="body" element="nwcg:ServicePingReq"/>
	</message>
	<message name="ServicePingResp">
		<part name="body" element="nwcg:ServicePingResp"/>
	</message>	
	<portType name="PingInterface">
		
		<operation name="ServicePing">
			<input message="tns:ServicePingReq"/>
			<output message="tns:ServicePingResp"/>
		</operation>

		
	</portType>
	<binding name="PingBinding" type="tns:PingInterface">
		<documentation>ResourceOrder Binding - All request messages must contain SecurityToken and MessageContext headers and one request message in the body</documentation>
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		

		<operation name="ServicePing">
			<soap:operation soapAction="ServicePing" style="document"/>
			<input>
				<soap:body use="literal"/>
			</input>
			<output>
				<soap:body use="literal"/>
			</output>
		</operation>

		

	</binding>
	<service name="PingService">
		<documentation>Only document/literal style SOAP messages sent over HTTPS are supported</documentation>
		<port name="PingPort" binding="tns:PingBinding">
			<soap:address location="http://localhost:9080/TestService/ResourceOrderService"/>
		</port>
	</service>
</definitions>
