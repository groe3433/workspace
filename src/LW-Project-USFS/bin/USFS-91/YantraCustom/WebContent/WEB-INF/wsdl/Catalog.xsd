<xs:schema xmlns:cat="http://nwcg.gov/services/ross/catalog/1.1" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:nwcg="http://nwcg.gov/services/ross/common_types/1.1" xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd" targetNamespace="http://nwcg.gov/services/ross/catalog/1.1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.1">
	<xs:import namespace="http://nwcg.gov/services/ross/common_types/1.1" schemaLocation="RossCommon.xsd"/>
	<xs:annotation>
		<xs:documentation>****************  Release 2.8 Catalog Message Definitions  **************</xs:documentation>
	</xs:annotation>
	<xs:element name="CreateCatalogItemReq">
		<xs:annotation>
			<xs:documentation>Used to create a catalog item</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MessageOriginator" type="nwcg:MessageOriginatorType">
					<xs:annotation>
						<xs:documentation>Identifies the system originating this request message</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CatalogItem" type="nwcg:CatalogItemCreateType">
					<xs:annotation>
						<xs:documentation>Information needed to create the catalog item</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CreateCatalogItemResp">
		<xs:annotation>
			<xs:documentation>Response when creating a catalog item</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ResponseStatus" type="nwcg:ResponseStatusType">
					<xs:annotation>
						<xs:documentation>Indicates the Result of the operation - Structure containing response status data returned to external systems</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CatalogItemKey" type="nwcg:CatalogItemNaturalKeyType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The key of the catalog item that was created</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GetCatalogItemReq">
		<xs:annotation>
			<xs:documentation>Used to get an existing catalog item</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MessageOriginator" type="nwcg:MessageOriginatorType">
					<xs:annotation>
						<xs:documentation>Identifies the system originating this request message</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CatalogItemKey" type="nwcg:CatalogItemNaturalKeyType">
					<xs:annotation>
						<xs:documentation>Key data of the catalog item to get</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GetCatalogItemResp">
		<xs:annotation>
			<xs:documentation>Response when getting a catalog item</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ResponseStatus" type="nwcg:ResponseStatusType">
					<xs:annotation>
						<xs:documentation>Indicates the Result of the operation - Structure containing response status data returned to external systems</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CatalogItem" type="nwcg:CatalogItemReturnType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Details for the catalog item</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="UpdateCatalogItemReq">
		<xs:annotation>
			<xs:documentation>Used to update a catalog item</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MessageOriginator" type="nwcg:MessageOriginatorType">
					<xs:annotation>
						<xs:documentation>Identifies the system originating this request message</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CatalogItemKey" type="nwcg:CatalogItemNaturalKeyType">
					<xs:annotation>
						<xs:documentation>Key data of the catalog item to update</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CatalogItem" type="nwcg:CatalogItemUpdateType">
					<xs:annotation>
						<xs:documentation>Updated information for a catalog item</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="UpdateCatalogItemResp">
		<xs:annotation>
			<xs:documentation>Response when updating a catalog item</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ResponseStatus" type="nwcg:ResponseStatusType">
					<xs:annotation>
						<xs:documentation>Indicates the Result of the operation - Structure containing response status data returned to external systems</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CatalogItemKey" type="nwcg:CatalogItemNaturalKeyType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The key of the catalog item being updated.  If the name or code is being updated and the update was successful this will contain the new key, otherwise it will contain the old key</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DeleteCatalogItemReq">
		<xs:annotation>
			<xs:documentation>Used to delete a catalog item</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MessageOriginator" type="nwcg:MessageOriginatorType">
					<xs:annotation>
						<xs:documentation>Identifies the system originating this request message</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CatalogItemKey" type="nwcg:CatalogItemNaturalKeyType">
					<xs:annotation>
						<xs:documentation>Key data of the catalog item to delete</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DeleteCatalogItemResp">
		<xs:annotation>
			<xs:documentation>Response when deleting a catalog item</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ResponseStatus" type="nwcg:ResponseStatusType">
					<xs:annotation>
						<xs:documentation>Indicates the Result of the operation - Structure containing response status data returned to external systems</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CatalogItemKey" type="nwcg:CatalogItemNaturalKeyType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The key of the catalog item that was deleted</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
