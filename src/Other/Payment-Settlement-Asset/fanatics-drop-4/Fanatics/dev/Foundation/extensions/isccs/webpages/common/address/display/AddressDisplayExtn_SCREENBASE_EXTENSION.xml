<?xml version="1.0" encoding="UTF-8"?>
<ExtensionMetaData>
    <ScreenExtension extnClassName="AddressDisplayExtn"
        extnPackage="extn.common.address.display"
        originalScreen="isccs.common.address.display.AddressDisplay"
        originalScreenPackage="isccs.common.address.display" type="diffrential">
        <Items>
            <Image _sc_extensibility_operation="change"
                _sc_extensibility_referenceUId="imgEmailLabel" renderHidden="false"/>
            <Image _sc_extensibility_operation="change"
                _sc_extensibility_referenceUId="imgPhoneNo" renderHidden="false"/>
            <Label _sc_extensibility_operation="change"
                _sc_extensibility_referenceUId="lblName" label="$(_scSimpleBundle:blank)"/>
            <Label _sc_extensibility_moveOrder="8"
                _sc_extensibility_operation="insert"
                _sc_extensibility_position="before"
                _sc_extensibility_referenceUId="extn_Companyname"
                spanLabel="true" uId="extn_Company_Name">
                <BindingData>
                    <SourceBinding namespace="Address" path="PersonInfo.Company"/>
                </BindingData>
            </Label>
            <Label _sc_extensibility_moveOrder="1"
                _sc_extensibility_operation="insert"
                _sc_extensibility_position="after"
                _sc_extensibility_referenceUId="lblName"
                uId="extn_Companyname" value="$(_scSimpleBundle:extn_Company_Name)">
                <BindingData>
                    <SourceBinding namespace="Address" path="PersonInfo.Company"/>
                </BindingData>
            </Label>
            <Label _sc_extensibility_moveOrder="3"
                _sc_extensibility_operation="insert"
                _sc_extensibility_position="after"
                _sc_extensibility_referenceUId="lblCityStateZip"
                label="$(_scSimpleBundle:Email)" uId="extn_Email" value="$(_scSimpleBundle:Email)">
                <BindingData>
                    <SourceBinding namespace="CustomerAddress" path="PersonInfoBillTo.EMailID"/>
                </BindingData>
            </Label>
            <Label _sc_extensibility_moveOrder="6"
                _sc_extensibility_operation="insert"
                _sc_extensibility_position="before"
                _sc_extensibility_referenceUId="lblPostalCode"
                uId="extn_Phone" value="$(_scSimpleBundle:extn_Phone)">
                <BindingData>
                    <SourceBinding namespace="Address" path="PersonInfo.DayPhone"/>
                </BindingData>
            </Label>
            <Label _sc_extensibility_moveOrder="10"
                _sc_extensibility_operation="insert"
                _sc_extensibility_position="after"
                _sc_extensibility_referenceUId="lblCountry"
                renderHidden="true" uId="extn_Email_ID">
                <BindingData>
                    <SourceBinding namespace="Address" path="PersonInfo.EMailID"/>
                </BindingData>
            </Label>
            <Label _sc_extensibility_moveOrder="12"
                _sc_extensibility_operation="insert"
                _sc_extensibility_position="before"
                _sc_extensibility_referenceUId="lblDayPhone"
                renderHidden="true" uId="extn_Phone_Number">
                <BindingData>
                    <SourceBinding namespace="Address" path="PersonInfo.DayPhone"/>
                </BindingData>
            </Label>
        </Items>
    </ScreenExtension>
</ExtensionMetaData>
