    	<mashup cached=""
        description="This mashup is called to massage the output of getCompleteOrderLineList API called when loading the shipment tracking page. The output is parsed to identify the different statuses of line and based on te values multiple panels are painted in UI."
        endpoint="EP_ORDER"
        id="lineTrack_getCompleteOrderLineDetails_Output"
        mashuptype="XAPI" skipDataProvider="false" transactional="true">
        <classInformation name="com.ibm.isccs.order.mashups.SCCSOrderLineTrackingMashup"/>
        <API Name="getCompleteOrderLineDetails">
            <Input>
                <OrderLineDetail
                    DisplayLocalizedFieldInLocale="xml:CurrentUser:/User/@Localecode"
                    EvaluateBundleParentLines="N" OrderLineKey=""/>
            </Input>
            <Template>
                <OrderLine DeliveryMethod=""
                    IsBundleCompletelyShipped="" IsBundleParent=""
                    ItemGroupCode="" MaxLineStatusDesc=""
                    OrderLineKey="" ReshipParentLineKey=""
                    ReturnReasonCode="" Status="">
                    <Order DocumentType="" EnterpriseCode="" HoldFlag="" OrderNo=""/>
                    <PriceInfo Currency=""/>
                    <Item UnitOfMeasure=""/>
                    <OrderLineTranQuantity TransactionalUOM=""/>
                    <PersonInfoShipTo AddressID="" AddressLine1="" AddressLine2="" 
                    City="" Company="" Country="" CountryDesc="" DayPhone="" 
                    Department="" EMailID="" EveningPhone="" FirstName="" 
                    IsAddressVerified="" IsCommercialAddress="" 
                    LastName="" MiddleName="" MobilePhone="" PersonID="" 
                    PersonInfoKey="" State="" Suffix="" Title="" TitleDesc="" 
                    ZipCode="" isHistory=""/>
                    <OrderDates>
			<OrderDate ActualDate="" CommittedDate="" 
			DateTypeId="" ExpectedDate="" 
			OrderHeaderKey="" OrderLineKey="" 
			OrderReleaseKey="" RequestedDate=""/>
		    </OrderDates>
                    <LineTracking>
                        <LineExceptions>
                            <LineException ExceptionType="">
                                <OrderDelay DelayDate="" PromisedDate=""/>
                                <OrderHoldType HoldType=""
                                    LastHoldTypeDate=""
                                    OrderHeaderKey="" OrderLineKey=""/>
                                <StopDeliveryRequestDetail
                                    RequestedQty="" SuccessfulQty="">
                                    <StopDeliveryRequest RequestDate=""/>
                                </StopDeliveryRequestDetail>
                                <OrderLine Createts="" ItemGroupCode=""
                                    OrderedQty="" ReturnReason="">
                                    <Order OrderHeaderKey="" DocumentType="" EnterpriseCode="" OrderNo="" OrderDate="" DraftOrderFlag="" />
                                    <OrderLineTranQuantity OrderedQty="" TransactionalUOM=""/>
                                </OrderLine>
                            </LineException>
                        </LineExceptions>
                        <LineBreakups>
                            <LineBreakup Quantity="" Status="">
                                <OrderStatus Status="" StatusDate=""
                                    StatusDescription="" StatusQty="">
                                    <Details ExpectedShipmentDate=""/>
                                    <ShipNode Description=""/>
                                </OrderStatus>
                                <ReshipOrderLine OrderLineKey=""/>
                                <ShipNode Description="">
                                    <ShipNodePersonInfo/>
                                </ShipNode>
                                <ShipmentLine ShipmentKey="" ShipmentLineKey="">
                                    <Shipment ActualShipmentDate="" SCAC="" 
                                    CarrierServiceCode=""
                                    ExpectedDeliveryDate=""
                                    ExpectedShipmentDate=""
                                    ShipmentNo="" Status=""
                                    TrackingNo="" URL=""/>
                                </ShipmentLine>
                                <ContainerDetail ContainerDetailsKey="">
                                    <Container ContainerNo=""
                                    ShipmentContainerKey=""
                                    TrackingNo="" URL="">
                                    	<Extn ExtnReturnTrackingNo="" />
                                    </Container>
                                </ContainerDetail>
                                <PersonInfoShipTo AddressID="" AddressLine1="" AddressLine2="" 
                                City="" Company="" Country="" CountryDesc="" DayPhone="" 
                                Department="" EMailID="" EveningPhone="" FirstName="" 
                                IsAddressVerified="" IsCommercialAddress="" 
                                LastName="" MiddleName="" MobilePhone="" PersonID="" 
                                PersonInfoKey="" State="" Suffix="" Title="" TitleDesc="" 
                                ZipCode="" isHistory=""/>
                            </LineBreakup>
                        </LineBreakups>
                    </LineTracking>
                </OrderLine>
            </Template>
        </API>
        <APINamespace inputNS="getCompleteOrderLineDetails_input" outputNS="getCompleteOrderLineDetails_input"/>
        <Config>
            <!-- For all Exception / Information / StatusPanelMap elements the attribute 
					FulfillmentAlteration can be set as "Y" if each occurance of that scenario 
					should be considered as an alteration to the normal fulfillment pipeline. 
					Each occurance is added together and a sum is returned to the client in the 
					attribute AlterationCount. In ISCCS this value is used to set the customer 
					message and set text for screen readers. -->
            <ExceptionPanelMapping>
                <!-- ExceptionType is the type returned from the getCompleteOrderLineDetails 
						API. -->
                <Exception ExceptionType="Delay"
                    FulfillmentAlteration="Y"
                    PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackDelay" PanelMode=""/>
                <Exception ExceptionType="Hold"
                    FulfillmentAlteration="Y"
                    PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackHold" PanelMode=""/>
                <Exception ExceptionType="Reshipped"
                    FulfillmentAlteration="Y"
                    PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackReshipped" PanelMode=""/>
                <Exception ExceptionType="StopDelivery"
                    FulfillmentAlteration="Y"
                    PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackStopDelivery" PanelMode=""/>
                <Exception ExceptionType="Returned"
                    FulfillmentAlteration="N" PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackReturned"/>
            </ExceptionPanelMapping>
            <InfoPanelMapping>
                <!-- InformationType is one of the following: Reshipped, BundleParent, 
						Return -->
                <Information InformationType="Reship"
                    PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackReship" PanelMode=""/>
                <Information InformationType="BundleParent"
                    PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackBundleParent" PanelMode=""/>
            </InfoPanelMapping>
            <StatusPanelMapList>
                <!-- Statuses are evaluated Greater than or equal to FromStatus. and 
						Less than ToStatus -->
                <!-- Statuses are evaluated in the order they appear in this XML. -->
                <!-- Supported Child element names under StatusPanelMap are OrderLine, 
						Shipment -->
                <AllDeliveryMethods>
                    <!-- 1300 and 1400 are special statuses in this mashup for the PanelID 
							below. They will always be consolidated into a single Line Breakup. The Quantity 
							will be the current backordered quantity. the 1400 statuses will be used 
							on the UI to create a history of where the system has attempted to source 
							the product from. If there is no quantity in 1300 then the 1400 statuses 
							will be ignored completely. IF it is desired to have each backorder quantity 
							and backordered from node quantity seperated in its own panel you need to 
							create a custom panel and change the Panel ID below. -->
                    <StatusPanelMap DocumentType="SALES_ORDER"
                        FulfillmentAlteration="Y"
                        PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackBackordered" PanelMode="Backordered">
                        <!-- Backordered -->
                        <StatusRanges>
                            <OrderLine FromStatus="1300" ToStatus="1400"/>
                        </StatusRanges>
                    </StatusPanelMap>
                    <StatusPanelMap DocumentType="SALES_ORDER"
                        FulfillmentAlteration="Y"
                        PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackBackordered" PanelMode="FromNode">
                        <!-- Backordered from Node -->
                        <StatusRanges>
                            <OrderLine FromStatus="1400" ToStatus="1400"/>
                        </StatusRanges>
                    </StatusPanelMap>
                    <StatusPanelMap PanelID="">
                        <!-- Cancelled -->
                        <StatusRanges>
                            <OrderLine FromStatus="9000" ToStatus="9000"/>
                        </StatusRanges>
                    </StatusPanelMap>
                    <StatusPanelMap PanelID="">
                        <!-- Shorted -->
                        <StatusRanges>
                            <OrderLine FromStatus="9020" ToStatus="9020"/>
                        </StatusRanges>
                    </StatusPanelMap>
                </AllDeliveryMethods>
                <SHP>
                    <StatusPanelMap DocumentType="SALES_ORDER"
                        PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackShipping" PanelMode="Processing">
                        <!-- Shipped -->
                        <StatusRanges>
                            <OrderLine ToStatus="3700"/>
                        </StatusRanges>
                    </StatusPanelMap>
                    <StatusPanelMap DocumentType="SALES_ORDER"
                        PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackShipping" PanelMode="Complete">
                        <!-- Shipped -->
                        <StatusRanges>
                            <OrderLine FromStatus="3700"/>
                        </StatusRanges>
                    </StatusPanelMap>
                    <StatusPanelMap DocumentType="RETURN_ORDER"
                        PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackShipping" PanelMode="ReturnProcessing">
                        <!-- Shipped -->
                        <StatusRanges>
                            <OrderLine ToStatus="3900"/>
                        </StatusRanges>
                    </StatusPanelMap>
                    <StatusPanelMap DocumentType="RETURN_ORDER"
                        PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackShipping" PanelMode="ReturnComplete">
                        <!-- Shipped -->
                        <StatusRanges>
                            <OrderLine FromStatus="3900"/>
                        </StatusRanges>
                    </StatusPanelMap>
                </SHP>
                <PICK>
                    <StatusPanelMap DocumentType="SALES_ORDER"
                        PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackPickup" PanelMode="Processing">
                        <StatusRanges>
                            <!-- Included In Shipment -->
                            <OrderLine ToStatus="3700"/>
                            <!-- Ready For Customer Pick -->
                            <Shipment ToStatus="1100.70.06.30"/>
                        </StatusRanges>
                    </StatusPanelMap>
                    <StatusPanelMap DocumentType="SALES_ORDER"
                        PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackPickup" PanelMode="Ready">
                        <!-- Ready For Customer Pick -->
                        <StatusRanges>
                            <Shipment FromStatus="1100.70.06.30" ToStatus="1100.70.06.30"/>
                        </StatusRanges>
                    </StatusPanelMap>
                    <StatusPanelMap DocumentType="SALES_ORDER"
                        PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackPickup" PanelMode="Complete">
                        <StatusRanges>
                            <OrderLine FromStatus="3700" ToStatus="3700.01"/>
                        </StatusRanges>
                    </StatusPanelMap>
                    <StatusPanelMap DocumentType="RETURN_ORDER"
                        PanelID="isccs.shipment.shipmentTracking.lineStatePanels.ShipTrackPickup" PanelMode="ReturnComplete"/>
                </PICK>
            </StatusPanelMapList>
        </Config>
        <AlternateResourceIds>
            <AlternateResourceId altResourceId="ISCORD0001"/>
        </AlternateResourceIds>
    </mashup>
