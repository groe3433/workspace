                    scExtensibilityArrayItemId="extn_SourceNamespaces_8" value="extn_getFanCashReasonCode_output"/>
            </SourceNamespaces>
            <TargetNamespaces>
                <Target
                    description="This model will hold the fancash and fancash reason code."
                    isExtn="true"
                    scExtensibilityArrayItemId="extn_TargetNamespaces_2" value="extn_fancashModel"/>
            </TargetNamespaces>
        </Namespaces>
        <MashupRefs>
            <Init>
                <MashupRef extnType="ADD"
                    mashupId="cancelOrder_getCompleteOrderDetails"
                    mashupRefId="getCompleteOrderDetails" sourceNamespace="getCompleteOrderDetails_output"/>
                <MashupRef extnType="ADD"
                    mashupId="extn_getFanCashReasonCode"
                    mashupRefId="extn_getFanCashReasonCode" sourceNamespace="extn_getFanCashReasonCode_output"/>
            </Init>
        </MashupRefs>
    </ScreenExtension>
</ExtensionMetaData>
