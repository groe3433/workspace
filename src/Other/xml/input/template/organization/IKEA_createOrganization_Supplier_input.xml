<Organization
    CapacityKeptExternally="${Organization.CapacityKeptExternally}"
    CapacityOrganizationCode="${Organization.CapacityOrganizationCode}"
    CatalogOrganizationCode="${Organization.CatalogOrganizationCode}"
    InventoryKeptExternally="${Organization.InventoryKeptExternally}"
    InventoryOrganizationCode="${Organization.InventoryOrganizationCode}"
    InventoryPublished="${Organization.InventoryPublished}"
    IsLegalEntity="${Organization.IsLegalEntity}"
    LocaleCode="${Organization.LocaleCode}"
    Operation="${Organization.Operation}"
    OrganizationCode="${Organization.OrganizationCode}"
    OrganizationName="${Organization.OrganizationName}"
    ParentOrganizationCode="${Organization.ParentOrganizationCode}"
        PrimaryEnterpriseKey="${Organization.PrimaryEnterpriseKey}">
<CorporatePersonInfo
        City="${Organization.CorporatePersonInfo.City}"
        Country="${Organization.CorporatePersonInfo.Country}"
        State="${Organization.CorporatePersonInfo.State}"
        ZipCode="${Organization.CorporatePersonInfo.ZipCode}"/> 
<Node
