        NodeType="${Organization.Node.NodeType}"
        ProcureToShipAllowed="${Organization.Node.ProcureToShipAllowed}"
            ReceiptProcessingTime="${Organization.Node.ReceiptProcessingTime}"/> 
<OrgRoleList>
<OrgRole
            RoleKey="${Organization.OrgRoleList.OrgRole.RoleKey}"/> 
<OrgRole
            RoleKey="${Organization.OrgRoleList.OrgRole.2.RoleKey}"/> 
</OrgRoleList>
<EnterpriseOrgList>
  <OrgEnterprise
            EnterpriseOrganizationKey="${Organization.EnterpriseOrgList.OrgEnterprise.EnterpriseOrganizationKey}"/> 
  <OrgEnterprise
        EnterpriseOrganizationKey="${Organization.EnterpriseOrgList.OrgEnterprise.2.EnterpriseOrganizationKey}"/> 
</EnterpriseOrgList>
<Calendar
        CalendarDescription="${Organization.Calendar.CalendarDescription}"
        CalendarId="${Organization.Calendar.CalendarId}"
                OrganizationCode="${Organization.Calendar.OrganizationCode}">
<EffectivePeriods>
<EffectivePeriod
                EffectiveFromDate="${Organization.Calendar.EffectivePeriods.EffectivePeriod.EffectiveFromDate}"
                        EffectiveToDate="${Organization.Calendar.EffectivePeriods.EffectivePeriod.EffectiveToDate}">
<Shifts>
  <Shift
                        FridayValid="${Organization.Calendar.EffectivePeriods.EffectivePeriod.Shifts.Shift.FridayValid}"
                        MondayValid="${Organization.Calendar.EffectivePeriods.EffectivePeriod.Shifts.Shift.MondayValid}"
                        SaturdayValid="${Organization.Calendar.EffectivePeriods.EffectivePeriod.Shifts.Shift.SaturdayValid}"
                        ShiftEndTime="${Organization.Calendar.EffectivePeriods.EffectivePeriod.Shifts.Shift.ShiftEndTime}"
                        ShiftName="${Organization.Calendar.EffectivePeriods.EffectivePeriod.Shifts.Shift.ShiftName}"
                        ShiftStartTime="${Organization.Calendar.EffectivePeriods.EffectivePeriod.Shifts.Shift.ShiftStartTime}"
                        SundayValid="${Organization.Calendar.EffectivePeriods.EffectivePeriod.Shifts.Shift.SundayValid}"
                        ThursdayValid="${Organization.Calendar.EffectivePeriods.EffectivePeriod.Shifts.Shift.ThursdayValid}"
                        TuesdayValid="${Organization.Calendar.EffectivePeriods.EffectivePeriod.Shifts.Shift.TuesdayValid}" WednesdayValid="${Organization.Calendar.EffectivePeriods.EffectivePeriod.Shifts.Shift.WednesdayValid}"/> 
  </Shifts>
  </EffectivePeriod>
  </EffectivePeriods>
