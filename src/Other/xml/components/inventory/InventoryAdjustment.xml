<?xml version="1.0" encoding="UTF-8"?>
<Component xmlns="http://www.lightwellinc.com/stf-component.xsd" xmlns:p1="http://www.lightwellinc.com/stf-common.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.lightwellinc.com/stf-component.xsd ../../schemas/SterlingTestFramework-Component.xsd " Name="IKEA_InventoryAdjustment_Track" Active="Y"
           Description="IKEA Inventory Adjustment pushing message to queue then calling FindInventory to confirm change">
	<Steps>
		<Sequence Number="1" Delay="4">
			<p1:Queue Name="SOM_IN_INVENTORY_ADJUST_QUEUE" Type="QUEUE_SEND" JmsServer="WL_JMS"></p1:Queue>
			<p1:Documents>
				<p1:Document Type="INPUT_DOCUMENT" FileName="IKEA_ItemManagement_ItemManagement.xml" Location="resources/xml/input/IKEA/InventoryAdjustment/">
				</p1:Document>
			</p1:Documents>
		</Sequence>
		<Sequence Number="2">
			<p1:Api Name="getInventoryAudit"></p1:Api>
			<p1:Documents>
				<p1:Document Type="INPUT_DOCUMENT" FileName="IKEA_InventoryAdjustment_getInventoryAudit_input.xml" Location="resources/xml/input/IKEA/InventoryAdjustment/">
				</p1:Document>
				<p1:Document Type="OUTPUT_TEMPLATE" FileName="IKEA_InventoryAdjustment_getInventoryAudit_input.xml" Location="resources/xml/output/IKEA/InventoryAdjustment/"></p1:Document>
			</p1:Documents>
			<p1:Validations>
				<p1:Validate ValidationType="XPATH" Expected="${Item.Quantity}" Path="/InventoryAudits/InventoryAudit/@Quantity">
				</p1:Validate>
			</p1:Validations>
			<p1:Parameters></p1:Parameters>
		</Sequence>
	</Steps>
	<Parameters/>
</Component>
