<DBSchema Name="CustomDBExtensions"> 
  <Entities> 
	<Entity TableName="EXTN_FAN_EMAIL" TableType="MASTER" Prefix="EXTN" HasHistory="Y" Description="Custom Table to store the email messages that will be published to queues" AuditRequired="N" ApiNeeded="Y">
		<Attributes>
			<Attribute Description="primary key for the custom table" XMLName="EmailKey" Nullable="False" DefaultValue="' '" DataType="Key" ColumnName="EMAIL_KEY"/>
			<Attribute Description="Key of the Order" XMLName="OrderHeaderKey" Nullable="False" DefaultValue="' '" DataType="Key" ColumnName="ORDER_HEADER_KEY"  ParentColumnName="ORDER_HEADER_KEY"/>			
			<Attribute Description="Organization Code" XMLName="OrganizationCode" Nullable="False" DataType="Text-200" ColumnName="ORGANIZATION_CODE"/>
			<Attribute Description="Order No" XMLName="OrderNo" Nullable="False" DataType="Text-40" ColumnName="ORDER_NO"/>
			<Attribute Description="Email ID of the Receipient" XMLName="EmailID" Nullable="False" DefaultValue="' '" DataType="Text-50" ColumnName="EMAIL_ID"/>
			<Attribute Description="Type of Email to be Sent" XMLName="EmailType" Nullable="False" DataType="Text-40" ColumnName="EMAIL_TYPE"/>
			<Attribute Description="The message XML" Nullable="False" DefaultValue="' '" DataType="CLOB" ColumnName="MSG_XML" XMLName="MsgXml"/>
			<Attribute Description="Status of the email process" XMLName="EmailStatus" Nullable="False" DataType="Text-20" ColumnName="EMAIL_STATUS"/>
			<Attribute Description="Create TimeStamp" DefaultValue="sysdate" DataType="TimeStamp" ColumnName="CREATETS"/>
			<Attribute Description="Modify TimeStamp" DefaultValue="sysdate" DataType="TimeStamp" ColumnName="MODIFYTS"/>
			<Attribute Description="Creating User ID" DefaultValue="' '" DataType="UserId" ColumnName="CREATEUSERID"/>
			<Attribute Description="Modifying User ID" DefaultValue="' '" DataType="UserId" ColumnName="MODIFYUSERID"/>
			<Attribute Description="Creating Program ID" DefaultValue="' '" DataType="ProgramID" ColumnName="CREATEPROGID"/>
			<Attribute Description="Modifying Program ID" DefaultValue="' '" DataType="ProgramID" ColumnName="MODIFYPROGID"/>
			<Attribute Description="Lock ID" DefaultValue="0" DataType="Lockid" ColumnName="LOCKID"/>
			<Attribute Description="Key of the specific event" XMLName="EntityKey" Nullable="True" DefaultValue="' '" DataType="Key" ColumnName="ENTITY_KEY"/>
			<Attribute Description="Event short desc" XMLName="EntityName" Nullable="True" DefaultValue="' '" DataType="Text-40" ColumnName="ENTITY_NAME"/>
		</Attributes>
		  <Parent ParentTableName="YFS_ORDER_HEADER" XMLName="YFSOrderHeader" >
            <Attribute ColumnName="ORDER_HEADER_KEY" ParentColumnName="ORDER_HEADER_KEY" />
         </Parent> 
		<!-- PrimaryKey is a mandatory attribute in entity definition. This element can have ONLY ONE attribute element -->
		<PrimaryKey Name="FAN_EMAIL_PK">
			<Attribute ColumnName="EMAIL_KEY" XMLName="EmailKey"/>
		</PrimaryKey>
	</Entity>
  </Entities> 
</DBSchema>
