		</p:Sequence>
		<p:Sequence Number="3">
			<p1:Api Name="getOrganizationHierarchy"></p1:Api>
			<p1:Documents>
				<p1:Document Type="INPUT_DOCUMENT" FileName="IKEA_getOrganization_input.xml"
					Location="resources/xml/input/template/organization/">
				</p1:Document>
				<p1:Document Type="OUTPUT_TEMPLATE" FileName="IKEA_getOrganizationHierarchy_output.xml"
					Location="resources/xml/output/template/organization/">
				</p1:Document>
			</p1:Documents>
			<p1:Validations>
			<p1:Validate ValidationType="XPATH" Expected="PE2 9EV" Path="/Organization/CorporatePersonInfo/@ZipCode"> </p1:Validate>
			</p1:Validations>
		</p:Sequence>
	</p:Steps>
  <p:Parameters>
	<p:GeneratedParameter Prefix="CDC.269-" Suffix="" Key="Organization.OrganizationCode" Type="INPUT"/>
	<p:Parameter Key="Organization.Calendar.OrganizationCode" Type="INPUT" Value="${Organization.OrganizationCode}"/>
	<p:Parameter Key="Organization.Calendar.2.OrganizationCode" Type="INPUT" Value="${Organization.OrganizationCode}"/>
	<p:Parameter Key="Organization.Calendar.CalendarId" Type="INPUT" Value="${Organization.OrganizationCode}"/>
	<p:Parameter Key="Organization.Calendar.2.CalendarId" Type="INPUT" Value="${Organization.OrganizationCode}"/>
