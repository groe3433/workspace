<?xml version="1.0" encoding="UTF-8"?>
<p:TestCase xmlns:p="http://www.lightwellinc.com/stf-test.xsd" 
	xmlns:p1="http://www.lightwellinc.com/stf-common.xsd" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.lightwellinc.com/stf-test.xsd ../../../schemas/SterlingTestFramework-Test.xsd "
	Active="Y" Name="IKEA_Organization_ModifyCDC_UT_009" Description="Create and modify CDC with proper input data.">
	<p:Steps>
		<!-- add the organization -->
		<p:Sequence Number="1">
			<p1:Api Name="createOrganizationHierarchy"></p1:Api>
			<p1:Documents>
				<p1:Document Type="INPUT_DOCUMENT" FileName="IKEA_createOrganization_CDC_input.xml"
					Location="resources/xml/input/template/organization/">
				</p1:Document>
			</p1:Documents>
		</p:Sequence>
		<p:Sequence Number="2">
			<p1:Api Name="modifyOrganizationHierarchy"></p1:Api>
			<p1:Documents>
				<p1:Document Type="INPUT_DOCUMENT" FileName="IKEA_createOrganization_CDC_input.xml"
					Location="resources/xml/input/template/organization/">
				</p1:Document>
			</p1:Documents>
			<p1:Parameters>
				<p1:Parameter Key="Organization.CorporatePersonInfo.ZipCode" Type="INPUT" Value="PE2 9EV" /> 
