<?xml version="1.0" encoding="UTF-8"?>
<Component xmlns="http://www.lightwellinc.com/stf-component.xsd"
	xmlns:p1="http://www.lightwellinc.com/stf-common.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.lightwellinc.com/stf-component.xsd ../../schemas/SterlingTestFramework-Component.xsd "
	Name="IKEA_ItemManagement_CreateSingleItem_Component" Active="Y" Description="IKEA ItemManagement pushing message to queue then calling getOrderList to confirm change">
	<Steps>
		<Sequence Number="1" >
			<p1:Api Name="manageItem" ></p1:Api>
			<p1:Documents>
				<p1:Document Type="INPUT_DOCUMENT" FileName="IKEA_ItemManagement_manageItem_input.xml" Location="resources/xml/input/template/item/">
				</p1:Document>
			</p1:Documents>
		</Sequence>
		<Sequence Number="2">
			<p1:Api Name="getItemList"></p1:Api>
			<p1:Documents>
				<p1:Document Type="INPUT_DOCUMENT" FileName="IKEA_ItemManagement_getItemList_input.xml" Location="resources/xml/input/template/item/">
				</p1:Document>
			</p1:Documents>
			<p1:Validations>
				<p1:Validate ValidationType="XPATH"
					Expected="${Item.ItemID}" Path="/ItemList/Item/@ItemID">
				</p1:Validate>
				<p1:Validate ValidationType="XPATH"
					Expected="${Item.PrimaryInformation.Description}" Path="/ItemList/Item/PrimaryInformation/@Description">
				</p1:Validate>
			</p1:Validations>
			<p1:Parameters></p1:Parameters>
		</Sequence>
	</Steps>
	<Parameters/>
</Component>