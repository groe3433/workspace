<?xml version="1.0" encoding="utf-8"?>
<definitions xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:nwcg="http://nwcg.gov/services/ross/common_types/1.1" xmlns:tns="http://nwcg.gov/services/ross/common_types/wsdl/1.1" xmlns:ns="http://nwcg.gov/services/ross/common_types/1.1" xmlns:ns1="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd" name="ExternalAuth_WSDL" targetNamespace="http://nwcg.gov/services/ross/common_types/wsdl/1.1">
	<types>
		<xs:schema version="1.1">
			<xs:import namespace="http://nwcg.gov/services/ross/common_types/1.1" schemaLocation="RossCommon.xsd"/>
			<xs:import namespace="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd" schemaLocation="RossSecurity.xsd"/>
		</xs:schema>
	</types>
	<message name="VerifySecuritySessionReq">
		<part name="body" element="nwcg:VerifySecuritySessionReq"/>
	</message>
	<message name="VerifySecuritySessionResp">
		<part name="body" element="nwcg:VerifySecuritySessionResp"/>
	</message>
	<message name="VerifySecuritySessionHeaderMessage">
		<part name="SecurityToken" element="wsse:Security"/>
		<part name="MessageContext" element="nwcg:MessageContext"/>
	</message>
	<portType name="VerifySecuritySessionInterface">
		<documentation>VerifySecuritySessionInterface PortType - operation response messages returned for synchronous operation</documentation>
		<operation name="VerifySecuritySessionId">
			<input message="tns:VerifySecuritySessionReq"/>
			<output message="tns:VerifySecuritySessionResp"/>
		</operation>
	</portType>
	<binding name="VerifySecuritySessionBinding" type="tns:VerifySecuritySessionInterface">
		<documentation>Verify Security Session Binding - All request messages must contain SecurityToken and MessageContext headers and one request message in the body</documentation>
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<operation name="VerifySecuritySessionId">
			<soap:operation soapAction="VerifySecuritySessionId" style="document"/>
			<input>
				<soap:header message="tns:VerifySecuritySessionHeaderMessage" part="SecurityToken" use="literal"/>
				<soap:header message="tns:VerifySecuritySessionHeaderMessage" part="MessageContext" use="literal"/>
				<soap:body parts="body" use="literal"/>
			</input>
			<output>
				<soap:body use="literal"/>
			</output>
		</operation>
	</binding>
	<service name="VerifySecuritySessionService">
		<documentation>VerifySecuritySessionPort supports only SOAP 1.0 document/literal style messages posted via HTTPS. See additional documentation in WSDL text.</documentation>
		<port name="VerifySecuritySessionPort" binding="tns:VerifySecuritySessionBinding">
			<documentation>Integration Test Endpoint</documentation>
			<soap:address location="https://drain.lw-lmco.com:15558/soap/ross"/>
		</port>
	</service>
</definitions>
