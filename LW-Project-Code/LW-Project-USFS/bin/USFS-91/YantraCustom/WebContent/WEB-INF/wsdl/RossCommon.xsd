				<xs:annotation>
					<xs:documentation>The place name of the incident if applicable.  Only provided if the value is being modified</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncidentLocation" type="nwcg:LocationCreateType" minOccurs="0">
