			</xs:element>
			<xs:element name="ROSSQualifiedResourceID" type="nwcg:CompositeResourceKeyType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Set of resources being returned that are available, active and qualified to fill the request</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ResourceRequestDetailsNotificationType">
