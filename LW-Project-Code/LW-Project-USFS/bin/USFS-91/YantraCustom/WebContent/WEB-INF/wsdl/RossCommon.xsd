	<xs:complexType name="IncidentNumberType">
		<xs:annotation>
			<xs:documentation>Structure containing Incident Attributes that comprise the Incident Number</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="HostID" type="nwcg:UnitIDType">
				<xs:annotation>
					<xs:documentation>Unit ID of the Incident Host Organization</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SequenceNumber" type="nwcg:IncidentSequenceNumberSimpleType">
				<xs:annotation>
					<xs:documentation>1 to 6 digit number that is used in forming the last portion of the incident number – Example: If Incident number is ‘AB-LD1-000012’, then the Sequence Number is 12.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="YearCreated">
				<xs:annotation>
					<xs:documentation>String value of year.  Example: ‘2006’</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
						<xs:pattern value="20[0-9]{2}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IncidentRadioFrequencyNaturalKeyType">
		<xs:annotation>
			<xs:documentation>Structure containing attributes that uniquely identify an incident radio frequency</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IncidentKey" type="nwcg:CompositeIncidentKeyType">
				<xs:annotation>
					<xs:documentation>Key information identifying the incident that the radio frequency is associated with</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FrequencyType" type="nwcg:RadioFrequencyTypeSimpleType">
				<xs:annotation>
					<xs:documentation>Enumeration to describe the type of frequency.  Example:  Air to Air Transmit, Air Tactics  etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FrequencyValue" type="nwcg:FrequencyValueSimpleType">
				<xs:annotation>
					<xs:documentation>Radio Frequency value</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Tone" type="nwcg:FrequencyToneSimpleType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Entered as three digits, a decimal, and two digits.  Tone is required when another frequency associated with the same incident has the same frequency type and frequency value (otherwise the tone is optional).  </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IncidentRadioFrequencyCreateType">
		<xs:annotation>
			<xs:documentation>Structure containing Incident Radio Frequency attributes used during creation</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FrequencyValue" type="nwcg:FrequencyValueSimpleType">
				<xs:annotation>
					<xs:documentation>Radio Frequency value</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FrequencyType" type="nwcg:RadioFrequencyTypeSimpleType">
				<xs:annotation>
					<xs:documentation>Enumeration to describe the type of frequency.  Example:  Air to Air Transmit, Air Tactics  etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsPrimaryIndicator" type="xs:boolean" default="false">
