	<xs:complexType name="AttributeChangedType">
		<xs:annotation>
			<xs:documentation>Facilitates the sending of changed attributes only during an update notification.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EntityType">
				<xs:annotation>
					<xs:documentation>Enumeration describing what changed: Resource, Incident, ResourceRequest etc (question regarding necessity of this element.  Is it redundant with choices below?)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="nwcg:EntityTypeSimpleType">
							<xs:attribute name="EntityTypeName" use="required">
								<xs:annotation>
									<xs:documentation>Enumeration of permitted entity names (values)</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Resource"/>
										<xs:enumeration value="Incident"/>
										<xs:enumeration value="ResourceRequest"/>
										<xs:enumeration value="IncidentAviationHazard"/>
										<xs:enumeration value="IncidentRadioFrequency"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:choice>
				<xs:element name="IncidentAttributeType">
					<xs:annotation>
						<xs:documentation>Enumeration representing Incident Attributes  ex: IncidentName, IncidentHostID, IncidentSequenceNumber etc. </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="IncidentAttributeTypeName" use="required">
							<xs:annotation>
								<xs:documentation>Enumeration of permitted attribute names (values)</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="IncidentName"/>
									<xs:enumeration value="IncidentType"/>
									<xs:enumeration value="LocationName"/>
									<xs:enumeration value="OfficeReferenceNumber"/>
									<xs:enumeration value="InitialDateTime"/>
									<xs:enumeration value="Description"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="IncidentRadioFrequencyAttributeType">
					<xs:annotation>
						<xs:documentation>Enumeration representing Incident Radio Frequency Attributes ex:  FrequencyValue FrequencyType, etc.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="IncidentRadioFrequencyAttributeTypeName" use="required">
							<xs:annotation>
								<xs:documentation>Enumeration of permitted attribute names (values)</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="FrequencyValue"/>
									<xs:enumeration value="FrequencyType"/>
									<xs:enumeration value="IsPrimaryIndicator"/>
									<xs:enumeration value="Tone"/>
									<xs:enumeration value="IsTacticalIndicator"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="IncidentAviationHazardAttributeType">
					<xs:annotation>
						<xs:documentation>Enumeration representing Incident Aviation Hazard Attributes ex: HazardType, City, State, etc. </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="IncidentAviationHazardAttributeTypeName" use="required">
							<xs:annotation>
								<xs:documentation>Enumeration of permitted attribute names (values)</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="HazardType"/>
									<xs:enumeration value="City"/>
									<xs:enumeration value="State"/>
									<xs:enumeration value="Description"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="ResourceRequestAttributeType">
					<xs:annotation>
						<xs:documentation>Enumeration representing ResourceRequest Attributes  ex: ResourceRequestInclusionType, ResourceRequestStatus  etc. </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="ResourceRequestAttributeTypeName" use="required">
							<xs:annotation>
								<xs:documentation>Enumeration of permitted attribute names (values)</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="NeedDateTime"/>
									<xs:enumeration value="RequestedItemName"/>
									<xs:enumeration value="InclusionType"/>
									<xs:enumeration value="SpecialNeedsIndicator"/>
									<xs:enumeration value="NamedRequestIndicator"/>
									<xs:enumeration value="Priority"/>
									<xs:enumeration value="MethodForPerson"/>
									<xs:enumeration value="MethodForOrganization"/>
									<xs:enumeration value="Description"/>
									<xs:enumeration value="Date"/>
									<xs:enumeration value="SystemName"/>
									<xs:enumeration value="DispatchUnitID"/>
									<xs:enumeration value="Author"/>
									<xs:enumeration value="Text"/>
									<xs:enumeration value="Name"/>
									<xs:enumeration value="LocationType"/>
									<xs:enumeration value="State"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="ResourceAttributeType">
					<xs:annotation>
						<xs:documentation>Enumeration representing Resource Attributes  ex: ResourceName</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="ResourceAttributeTypeName" use="required">
							<xs:annotation>
								<xs:documentation>Enumeration of permitted attribute names (values)</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string"/>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="NewAttributeValue" type="xs:string">
				<xs:annotation>
					<xs:documentation>string rep of new attribute value</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
