			<xs:extension base="nwcg:ApplicationSystemType"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="TownshipRangeSectionCoordinateType">
		<xs:annotation>
			<xs:documentation>Structure containing location data in Township Range Section (TRS) format</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="State">
				<xs:annotation>
					<xs:documentation>State portion of the meridian - 2 character state code</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="2"/>
						<xs:enumeration value="AK"/>
						<xs:enumeration value="AL"/>
						<xs:enumeration value="AR"/>
						<xs:enumeration value="AZ"/>
						<xs:enumeration value="CA"/>
						<xs:enumeration value="CO"/>
						<xs:enumeration value="FL"/>
						<xs:enumeration value="IA"/>
						<xs:enumeration value="ID"/>
						<xs:enumeration value="IN"/>
						<xs:enumeration value="IL"/>
						<xs:enumeration value="KS"/>
						<xs:enumeration value="LA"/>
						<xs:enumeration value="MI"/>
						<xs:enumeration value="MN"/>
						<xs:enumeration value="MO"/>
						<xs:enumeration value="MS"/>
						<xs:enumeration value="MT"/>
						<xs:enumeration value="ND"/>
						<xs:enumeration value="NE"/>
						<xs:enumeration value="NM"/>
						<xs:enumeration value="NV"/>
						<xs:enumeration value="OK"/>
						<xs:enumeration value="OR"/>
						<xs:enumeration value="SD"/>
						<xs:enumeration value="UT"/>
						<xs:enumeration value="WA"/>
						<xs:enumeration value="WI"/>
						<xs:enumeration value="WY"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MeridianName" type="nwcg:MeridianSimpleType">
				<xs:annotation>
					<xs:documentation>Enumeration representing supported meridians</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Township">
				<xs:annotation>
					<xs:documentation>6 mile intervals east and west of the principal meridian</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="6"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TownshipDirection">
				<xs:annotation>
					<xs:documentation>Township value is which direction relative to the principal meridian</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="North"/>
						<xs:enumeration value="South"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Range">
				<xs:annotation>
					<xs:documentation>6 mile intervals north and south of a base line that is associated with each principal meridian</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="6"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="RangeDirection">
				<xs:annotation>
					<xs:documentation>Range value is which direction relative to the principal meridian</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="East"/>
						<xs:enumeration value="West"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Section">
				<xs:annotation>
					<xs:documentation>Value between 1 and 36 indicating which 1/36th of the Township</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="QuarterName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If the location is within a known township/range, the name of the quarter or quarter-quarter (i.e. SE NW).</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
