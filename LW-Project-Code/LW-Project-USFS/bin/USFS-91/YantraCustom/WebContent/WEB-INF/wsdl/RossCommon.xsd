					<xs:documentation>The set of radio frequencies associated with the incident.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncidentDocumentation" type="nwcg:DocumentationTextSimpleType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Incident Documentation Text</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncidentAviationHazard" type="nwcg:IncidentAviationHazardCreateType" minOccurs="0" maxOccurs="unbounded">
