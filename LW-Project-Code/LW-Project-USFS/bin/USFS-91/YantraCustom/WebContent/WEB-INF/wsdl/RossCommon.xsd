	<xs:complexType name="ResourceRequestPendingReturnType">
		<xs:annotation>
			<xs:documentation>Structure containing pending resource request information returned to external systems</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RequestKey" type="nwcg:CompositeResourceRequestKeyType">
				<xs:annotation>
					<xs:documentation>Key information indicating which resource request is being returned</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RequestBase" type="nwcg:ResourceRequestBaseReturnType">
				<xs:annotation>
					<xs:documentation>Resource request information that is common to both pending and filled requests</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ROSSQualifiedResourcesCount" type="xs:int">
